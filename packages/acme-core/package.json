{
  "name": "@acme/core",
  "type": "module",
  "version": "0.0.0",
  "imports": {
    "#/*": "./src/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsdown src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsdown src/index.tsx --format esm,cjs --watch --dts --external react",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "tsdown": "^0.20.1",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
